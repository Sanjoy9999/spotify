{
  "version": 2,
  "routes": [
    {
      "src": "/songs/(.*)\\.mp3$",
      "headers": {
        "content-type": "audio/mpeg"
      },
      "dest": "/songs/$1.mp3"
    },
    {
      "src": "/songs/(.*)\\.jpeg$",
      "headers": {
        "content-type": "image/jpeg"
      },
      "dest": "/songs/$1.jpeg"
    },
    {
      "src": "/songs/(.*)",
      "dest": "/songs/$1",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "cleanUrls": true
}
