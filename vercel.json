{
  "version": 2,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/songs/(.*)\\.mp3",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        }
      ]
    },
    {
      "source": "/songs/(.*)\\.jpeg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/songs/:path*",
      "destination": "/songs/:path*"
    }
  ]
}
